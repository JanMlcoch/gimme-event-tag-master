<h1>{{loginTitle}}</h1>
<form>
    <table>
        <tr>
            <td>{{loginText}}
            <td>
            <td><input type='text' id='login'/></td>
        </tr>
        <tr>
            <td>{{passText}}
            <td>
            <td><input type='password' id='password'/></td>
        </tr>
        <tr>
            <td><input type='button' id='submitCredentials' value='{{submitText}}'></td>
        </tr>
    </table>
</form>